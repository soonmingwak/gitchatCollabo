<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bbs.mappers.bbs">
	
	<!-- 로그인 SQL -->
	<select id="login" resultType="Member">
		SELECT *
		FROM   Member
		WHERE  m_id = #{m_id}
		AND    m_pw = #{m_pw}
	</select>
		
</mapper>

